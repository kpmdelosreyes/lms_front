		<!-- aside col_1 -->
		<div class="aside col_1">
			<!-- login -->
			<div class="login">
				<h2>Login</h2>
				<form action="#" method="post">
					<div class="group_username">
						<input type="text" name="login_usernname" id="login_username" />
						<input type="checkbox" name="remember_username" id="remember_username" value="1"/> <label for="remember_username">Remember Username</label>
					</div>
					<div class="group_password">
						<input type="password" name="login_password" id="login_password" />
						<input type="checkbox" name="remember_password" id="remember_password" value="2"/> <label for="remember_password">Remember Password</label> 
					</div>
					<div class="group_submit">
						<input type="submit" value="Login" id="login_submit"/> 
						Forgot <a href="#" class="finding_id">ID</a> / <a href="#" class="finding_password">Password</a> <span class="register"><a href="#" >Register</a></span>					
					</div>				
				</form>
				<div class="login_apply">
					<a href="#" class="apply_class"><span><img src="images/icon_apply_class.png"/>Apply for Class</span></a>
					<a href="#" class="apply_test"><span><img src="images/icon_apply_test.png"/>Apply for Level Test</span></a>		
				</div>
			</div>
			<!-- //login -->
			<!-- members -->
			<div class="members">
				<h2>English Course</h2>
				<ul>
