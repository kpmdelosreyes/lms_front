<link rel="stylesheet" type="text/css" href="css/style.css" />
<link rel="stylesheet" type="text/css" href="css/message_style.css" />
<script type="text/javascript" src="js/jquery-1.5.min.js"></script>
<script type="text/javascript" src="js/jquery.jcarousel.js"></script>
<script type="text/javascript" src="js/newscarousel.js"></script>
<script type="text/javascript" src="js/scripts.js"></script>
</head> 
<body>
<div id="wrap"> 
	<!-- HEADER --> 
	<div id="header">
		<!-- header_inner -->
		<div class="header_inner">
			<h1 class="logo"><a href="#"><img src="images/logo.png" alt="WebcafeLMS - Beacause learning is Life..."/></a></h1>
			<!-- header_content -->
			<div class="header_content">
				<!-- header_top -->
				<div class="header_top">
					<ul class="gnb">
						<li><a href="#">Special Event</a></li>
						<li><a href="#">Customer Service</a></li>
						<li><a href="#">Site Map</a></li>
						<li class="last"><a href="#">Login</a></li>
					</ul>				
				</div>
				<!-- //header_top -->
				<!-- mnb -->
				<ul class="mnb">
					<li><a href="#">English Course</a></li>
					<li><a href="#">U-Learning</a></li>
					<li><a href="#">Level Test</a></li>
					<li><a href="#">Apply Class</a></li>
					<li><a href="#">Forums</a></li>
					<li><a href="#">About Us</a></li>
				</ul>
				<!-- //mnb -->
			</div>
			<!-- //header_content -->
		</div>
		<!-- //header_inner -->
	</div>
	<!-- //HEADER -->
	<!-- CONTAINER -->
	<div id="container">
	<!-- container_wrapper -->
	<div class="container_wrapper">
	<!-- container_inner -->
	<div class="container_inner">
		<div class="heading">
			<div class="heading_top">
				<h2>[ My Page ]</h2>
				<p>You can check your schedule, do your homework, and attend your class. </p>		
			</div>
			<div class="heading_bottom">
				<ul class="breadcrumb">			
					<li><a href="#">Home</a></li>
					<li><a href="#">My Page</a></li>
					<li class="first">Write a message</li>
				</ul>
				<h2 class="title">Write a message</h2>				
			</div>
		</div>
		<!-- aside col_1 -->
		<div class="aside col_1">		
			<!-- my_class_nav -->
			<div class="my_class_nav lnb">
				<h2>My Class</h2>
				<ul>
					<li><a href="#">Class Schedule</a></li>
					<li><a href="#">Change Schedule History</a></li>
					<li><a href="#">Class Evaluation</a></li>
					<li><a href="#">Level Test</a></li>
					<li><a href="#">Class History</a></li>					
				</ul>
			</div>
			<!-- //my_class_nav -->
			<!-- ulearning_nav -->
			<div class="ulearning_nav lnb">
				<h2>uLearning</h2>
				<ul>
					<li><a href="#">uLearning Materials</a></li>
				</ul>
			</div>
			<!-- //ulearning_nav -->
			<!-- message_nav -->
			<div class="message_nav lnb">
				<h2>Message</h2>
				<ul>
					<li><a href="#">Recieved Message</a></li>
					<li class="current"><a href="#">Sent Message</a></li>
					<li><a href="#">1:1 Advise</a></li>
				</ul>
			</div>
			<!-- //message_nav -->
		</div>
		<!-- //aside col_1 -->
		<!-- CONTENT -->
		<div id="content">
			<div class="message_write_wrap">
				<!-- my page head title -->
				<h3 class="mypage_head_title"><span><a href="#">Message</a></span> Write a message</h3>
				<form method="get" action="#">
					<div class="message_reciptient">
						<label for="msg_to">To:</label>
						<input type="text" name="msg_to" id="msg_to" class="input_field3 no_focus input_style"/>
						<a href="#" class="btn_default btn_type_1 btn_type_2">Verify ID</a>
					</div>
					<div class="message_title">
						<label for="msg_title">Title:</label>
						<input type="text" name="msg_title" id="msg_title" maxlength="250" class="input_style input_field4 no_focus" />
					</div>
					<div class="message_text_area">
						<img src="images/dummy_textarea_bg.png" alt="dummy text">
						<textarea name="msg_text_area" id="msg_text_area" class="text_style text_field2 no_focus"></textarea>
					</div>
					<input type="submit" value="Submit" name="write_submit" id="write_submit" class="btn_default btn_type_2 fr"/>
					<a href="#" class="btn_default btn_type_2 fr">Back to list</a>
				</form>
			</div>
		</div>
		<!-- //CONTENT -->
	</div>
	<!-- //container_inner -->
		<!-- aside col_2 -->
		<div class="aside col_2">
			<div class="aside_inner">
				<!-- need_guide -->
				<div class="need_guide">
					<a href="#"><img src="images/img_need_guide.jpg" alt="Do you need a guide?"/></a>	
				</div>		
				<!-- //need_guide -->
				<!-- quick_menu -->			
				<div class="quick_menu">
					<h2>Quick Menu</h2>
					<ul class="rnb">
						<li class="current"><a href="#">My Page</a></li>
						<li><a href="#">Apply for Class</a></li>
						<li><a href="#">Apply for Level Test</a></li>
						<li><a href="#">U-Learning</a></li>
						<li><a href="#">About Course</a></li>
						<li><a href="#">About Class Materials</a></li>
					</ul>
				</div>
				<!-- //quick_menu -->
				<!-- banner_1 -->
				<div class="banner_1">
					<a href="#"><img src="images/img_banner_v1.jpg" alt="English is fun, We'll show you why"/></a>
				</div>
				<!-- //banner_1 -->
				<!-- banner_2 -->
				<div class="banner_2">
					<a href="#" class="left_banner_2"><img src="images/img_banner_v2.jpg" alt="left banner 2"/></a>
					<p>Nulla ipsum nunc, gravida tempus varius eu, eleifend non elit. Nam elementum iaculis lorem, et.</p>
				</div>
				<!-- //banner_2 -->
			</div>
		</div>
		<!-- //aside col_2 -->
	</div>
	<!-- //container_wrapper -->
	</div>
	<!-- //CONTAINER -->
	<!-- FOOTER -->
	<div id="footer">
		<div class="footer_inner">
			<div class="footer_content">
				<p><a href="#" class="terms">Terms of Use</a> <a href="#" class="privacy_policy">Privacy</a></p>
				<p class="copyright"><a href="#">WebcafeLMS</a> copyright &copy; 2012</p>				
			</div>
			<a href="#" class="footer_logo"><img src="images/logo_footer.png" alt="WebcafeLMS - Beacause learning is Life..."/></a>
		</div>
	</div>
	<!-- //FOOTER -->
</div> 
</body>
</html>